{
  "expo": {
    "name": "Flash Send",
    "slug": "FlashSend",
    "version": "5.1.1",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "flashsend",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "androidMode": "collapse",
      "androidCollapsedTitle": "New Messages"
    },
    "android": {
      "permissions": ["CAMERA", "RECORD_AUDIO", "NOTIFICATIONS"],
      "config": {
        "googleMaps": { "apiKey": "" }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flashsend",
      "googleServicesFile": "./env/google-services.json"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-font",
      [
        "expo-av",
        {
          "microphonePermission": "Allow expo-av to access your microphone."
        }
      ],
      [
        "expo-notifications",
        {
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "fe9b89da-f74b-4905-8d16-89fdb244fbfa"
      }
    }
  }
}
